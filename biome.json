{
	"$schema": "https://biomejs.dev/schemas/1.6.4/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"a11y": {
				"noSvgWithoutTitle": "off"
			},
			"recommended": true,
			"style": {
				"useConst": "off"
			},
			"correctness": {
				"noEmptyPattern": "off",
				"useExhaustiveDependencies": "off"
			},
			"suspicious": {
				"noExplicitAny": "off",
				"noEmptyInterface": "off"
			},
			"complexity": {
				"noBannedTypes": "off"
			}
		}
	}
}
